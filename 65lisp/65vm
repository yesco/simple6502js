OPTS=" -DPROGSIZE "
name="lisp"

# why doesn't this quote every arg in the resulting string
xx="$*"
echo "unix> ./65lisp $xx"

cl65 -O $OPTS -t sim6502 -DAL $name.c -o $name.out -m $name.map && (sim65 -c $name.out "$@" ; echo "--- EXIT=$? ---") | tee $name.log && (echo "`grep ' cycles' $name.log | sed -e 's/ cycles//'` /1024/1024" | bc -l ; echo "seconds simulated time")

echo
echo




