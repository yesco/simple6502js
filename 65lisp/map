perl map.pl $1.map | tee ident.sizes | grep '_' | grep -v '__' | grep -v "_main" | grep -v '_exit$' | grep -v "_initlisp"  > fil
echo "--- ORDER: $* ---"
cat fil
echo "--- TOP: $* ---"
cat fil | sort -r -n
echo "=== Functions: `tail +10 fil | wc -l ` (excluded 9: _interpret _error _sta _mathop _reset _nexttoken _nextloop _foobar _transtable)"
echo "=== Bytes    :`cat $1.bin | wc -c `"


