echo "(lambda () (+ foo 1000))" | ./run -q -E
echo "((lambda () (+ foo 1000)))" | ./run -q -E
echo "((lambda (foo) (+ foo 1000)) 77)" | ./run -q -E
echo "((lambda (a b) (+ a b) (* a b)) 11 22)" | ./run -q -E

echo "((lambda (n) 42))" | ./run -q -E
echo "((lambda (f n) (f n f)) (lambda (n) 42))" | ./run -q -E

echo "((lambda (f n) (funcall f 72)) (lambda (n) 42))" | ./run -q -E
