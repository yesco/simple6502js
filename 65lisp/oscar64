OPT="-Oz -Os" # 1684 Bytes  12,896,201 same -Os, smaller)

../../oscar64/bin/oscar64 $OPT -DNOFLOAT -DNOLONG -ep -gp $1.c ; ls -l $1.[ap]* ; wc $1.asm
